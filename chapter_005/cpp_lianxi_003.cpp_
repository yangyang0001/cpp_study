# include <iostream>
# include <climits>
# include <string>
# include <cstring>
# include <vector>
# include <array>
# include <ctime>


using namespace std;

int main() {
    cout.setf(ios_base::fixed, ios_base::floatfield);

    array<long double, 100> array;
    array[0] = 1;

    for(int i = 1; i <= 100; i++) {
        array[i] = i * array[i-1];
        cout << i << "! = " << "array[" << i << "] = " << array[i] << endl;
    }

    return 0;
}




