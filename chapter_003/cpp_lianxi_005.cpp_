#include <iostream>
#include <climits>

using namespace std;

int main() {
    cout.setf(ios_base::fixed, ios_base::floatfield);
    long long allPersons;
    long long usPersons;

    cout << "please enter allPersons:";
    cin >> allPersons;

    cout << "please enter usPersons :";
    cin >> usPersons;

    double percentage = (double) usPersons / (double) allPersons;

    cout << "percentage is :" << percentage << endl;
    return 0;
}

